class Student: {
    universityId private := 538;
    def constructor(this, sid, gpa, smart): {
        this.sid := sid + 1;
        this.gpa := gpa;
        this.smart := smart
    };
    def getID(this): {
        this.universityId
    }
};
goodStudent := Student(123, 4, true);
test(goodStudent.getID() = 538)