x := 0;
y := 0;
z := 0;
acc := 0;
while (y < 20) {
    if (y = 10) {break} else {acc := acc + y};
    while (z < 20) {
        if (z = 10) {break} else {acc := acc + z};
        while (x < 20) {
            if (x = 10) {break} else {acc := acc + x};
            x := x + 1
        };
        z := z + 1;
        x := 0
    };
    z := 0;
    y := y + 1
};
test (acc = 75780)